                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Nutrición</p>
                                    </a>
                                </li>

                                <li class="nav-item">
                                    <a asp-controller="Milkings" asp-action="Index" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Ordeño</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a asp-controller="ReprodPregnancy" asp-action="Index" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Embarazos</p>
                                    </a>
                                </li>
                                 <li class="nav-item">
                                    <a asp-controller="Insemination" asp-action="Index" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Inseminaciones</p>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li class="nav-item">
                            <a asp-controller="Bonus" asp-action="Index" class="nav-link">
                                <i class="far fa-circle nav-icon"></i>
                                <p>Calculo Aguinaldo</p>
                            </a>
                        </li>
                    <li class="nav-item">
                        <a asp-controller="Severance" asp-action="Index" class="nav-link">
